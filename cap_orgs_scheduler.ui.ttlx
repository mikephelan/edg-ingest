<http://capabilities.company.com/cap_orgs_scheduler>
  a owl:Ontology ;
  owl:imports <http://capabilities.company.com/cap_orgs> ;
  owl:imports <http://topbraid.org/scheduler> ;
  owl:imports <http://topbraid.org/swa> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
cap_orgs_scheduler:capOrgsScheduler
  a scheduler:ScheduledJob ;
  scheduler:cronSchedule "0 23 * * * ?" ;
  ui:prototype """
<ui:group>
    <ui:log ui:info=\"cap_orgs_scheduler start\"/>
    <ui:task ui:taskName=\"Capability Organizations Update\" ui:totalWork=\"{= 100 }\">
        <cap_orgs:updateCapOrgs/>
    </ui:task>
    <ui:log ui:info=\"cap_orgs_scheduler finish\"/>
</ui:group>
"""^^ui:Literal ;
  rdfs:label "EDS-ORG" ;
  rdfs:subClassOf scheduler:ScheduledJobs ;
.
